{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "functions": {
        "api/*": {
            "memory": 512,
            "maxDuration": 300
        }
    }
}